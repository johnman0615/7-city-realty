    "type-check": "npm run type-check --workspace=server && npm run type-check --workspace=client"
  },
  "devDependencies": {
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/react-router-dom": "^5.3.3",
