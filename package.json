{
  "name": "7-city-home-realty",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "clean": "rimraf dist && rimraf server/dist && rimraf client/dist",
